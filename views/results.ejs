<h1>Results Page</h1>

<% if (data.Response === "True") { -%>
	<ul>
		<% data["Search"].forEach(function(movie){%>
			<li><strong><%= movie["Title"]%></strong> 
				(<%= movie["Year"]%>)
			</li>
		<% });%>
	</ul>
<% } else { -%>
	<h3>No Matching Results</h3>
<% } -%>

<a href="/"><button name="return">Go Back To Search</button></a>